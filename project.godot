save/encryption_on_exports_only=true
text/autoadvance_fixed_delay=0.0
text/autoadvance_enabled=true
text/autopauses={}
variables={}
text/autoadvance_per_word_delay=0

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/virtual_joystick/plugin.cfg")

